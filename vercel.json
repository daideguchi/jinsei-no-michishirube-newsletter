{
  "buildCommand": "echo 'Build completed'",
  "outputDirectory": "./",
  "functions": {
    "api/signup.js": {
      "maxDuration": 30
    },
    "api/send-scheduled-emails.js": {
      "maxDuration": 60
    }
  },
  "crons": [
    {
      "path": "/api/send-scheduled-emails",
      "schedule": "0 10 * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}